<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="346.0" prefWidth="178.0" style="-fx-background-color: #b6e2f6;" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.finalprojdb.PatientMenuController">
    <children>
        <ImageView fitHeight="123.0" fitWidth="230.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="https://www.birzeit.edu/sites/default/files/social_content/facebook/open-graph_39.jpg" />
            </image>
        </ImageView>
        <ImageView fitHeight="22.0" fitWidth="217.0" pickOnBounds="true" preserveRatio="true" />
        <Button fx:id="profile" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#profile_buttonClicked" prefHeight="58.0" prefWidth="217.0" style="-fx-background-color: #b6e2f6;" text="Profile">
            <font>
                <Font name="Arabic Typesetting" size="26.0" />
            </font>
        </Button>
        <Button fx:id="clinicD" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#clinicD_buttonClicked" prefHeight="58.0" prefWidth="217.0" style="-fx-background-color: #b6e2f6;" text="Clinic doctors">
            <font>
                <Font name="Arabic Typesetting" size="26.0" />
            </font>
        </Button>
        <Button fx:id="reservation" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#reservation_buttonClicked" prefHeight="58.0" prefWidth="217.0" style="-fx-background-color: #b6e2f6;" text="Appointment reserver">
            <font>
                <Font name="Arabic Typesetting" size="26.0" />
            </font>
        </Button>
        <Button fx:id="patientsHistory" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#patientsHistory_buttonClicked" prefHeight="58.0" prefWidth="217.0" style="-fx-border-color:  #b6e2f6; -fx-background-color: #b6e2f6;" text="Patient's History">
            <font>
                <Font name="Arabic Typesetting" size="26.0" />
            </font>
        </Button>
        <Button fx:id="editPW" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#editPW_buttonClicked" prefHeight="58.0" prefWidth="217.0" style="-fx-border-color: #b6e2f6; -fx-background-color: #b6e2f6;" text="Reset password">
            <font>
                <Font name="Arabic Typesetting" size="26.0" />
            </font>
        </Button>
        <Button fx:id="contactInfo" alignment="BOTTOM_LEFT" mnemonicParsing="false" onAction="#contactInfo_buttonClicked" prefHeight="58.0" prefWidth="217.0" style="-fx-border-color:  #b6e2f6; -fx-background-color: #b6e2f6;" text="Contact Informations">
            <font>
                <Font name="Arabic Typesetting" size="26.0" />
            </font>
        </Button>
    </children>
</VBox>
